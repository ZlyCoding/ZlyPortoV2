{
    "version": 2,
    "name": "fazly-portfolio",
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/",
            "dest": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "github": {
        "silent": true
    }
}